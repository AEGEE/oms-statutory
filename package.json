{
  "name": "statutory",
  "version": "1.6.5",
  "description": "Statutory events module for MyAEGEE",
  "main": "lib/server.js",
  "scripts": {
    "lint": "eslint .",
    "db:create": "sequelize db:create",
    "db:setup": "sequelize db:drop; sequelize db:create; sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "test": "NODE_ENV=test npm run db:setup && jest test/api/*.js --runInBand --forceExit",
    "cli": "node --experimental-repl-await -r ./cli.js",
    "open-coverage": "open-cli coverage/lcov-report/index.html"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "models/**/*.js",
      "!lib/run.js",
      "!lib/sequelize.js",
      "!lib/logger.js",
      "!lib/fs.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [],
  "author": "Sergey Peshkov <github.com/serge1peshcoff>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AEGEE/statutory/issues"
  },
  "homepage": "https://github.com/AEGEE/statutory",
  "dependencies": {
    "@bugsnag/js": "^7.6.0",
    "body-parser": "^1.19.0",
    "bugsnag": "^2.4.3",
    "bunyan": "^1.8.15",
    "deep-assign": "^3.0.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "express-query-boolean": "^2.0.0",
    "file-type": "^16.2.0",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-range": "^4.0.2",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-schedule": "^2.0.0",
    "node-xlsx": "^0.16.1",
    "on-finished": "^2.3.0",
    "pg": "^8.5.1",
    "prom-client": "^13.1.0",
    "read-chunk": "^3.2.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rimraf": "^3.0.2",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "codecov": "^3.8.1",
    "conventional-changelog": "^3.1.24",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "faker": "^5.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "nock": "^13.0.7",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "semantic-release": "^17.3.7",
    "timekeeper": "^2.2.0"
  }
}
